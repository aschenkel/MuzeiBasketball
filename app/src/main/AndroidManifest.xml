<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
package="com.schenkel.axel.muzeibasket">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name=".Application.MyApplication"
        android:allowBackup="true"
        android:label="@string/app_name"
        android:icon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Transparent">

    <service android:name="com.schenkel.axel.muzeibasket.MuzeiImageGenerator"
        android:label="@string/source_name"
        android:description="@string/description"
        android:icon="@drawable/basketball"
        >
        <intent-filter>
            <action android:name="com.google.android.apps.muzei.api.MuzeiArtSource" />
        </intent-filter>

        <meta-data android:name="color" android:value="@color/orange" />
    </service>
        <activity android:name="com.schenkel.axel.muzeibasket.Permissions.AskForPermissionsActivity" />

</application>

</manifest>